<Window x:Class="service.ReturnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:service"
        mc:Ignorable="d"
        Title="Возврат" Height="104" Width="357" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid x:Name="ReturnWind">
        <TextBox x:Name="txtBox_name" HorizontalAlignment="Left" Margin="84,5,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="243" Height="25"/>
        <TextBox x:Name="txtBox_count" HorizontalAlignment="Left" Margin="84,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Height="25"/>
        <Label Content="Название" HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top"/>
        <Label Content="Количество" HorizontalAlignment="Left" Margin="5,36,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnCancel" Content="Отмена" HorizontalAlignment="Left" Margin="272,36,0,0" VerticalAlignment="Top" Height="25" Width="55" Click="btnCancel_Click"/>
        <Button x:Name="btnConfirm" Content="Подтвердить" HorizontalAlignment="Left" Margin="182,36,0,0" VerticalAlignment="Top" Width="82" Height="25" Click="btnConfirm_Click"/>

    </Grid>
</Window>
